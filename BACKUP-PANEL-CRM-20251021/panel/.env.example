# Configuración del Panel CRM

# Puerto del servidor (por defecto 3000)
PORT=3000

# Facebook Pixel ID
# Obtener en: https://business.facebook.com/events_manager2/list
FACEBOOK_PIXEL_ID=TU_PIXEL_ID_AQUI

# Facebook Access Token
# Obtener en: https://business.facebook.com/settings/system-users
# Recomendado: System User Token (no expira)
FACEBOOK_ACCESS_TOKEN=TU_ACCESS_TOKEN_AQUI

